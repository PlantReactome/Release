    <bp:TITLE rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fringe modulation of Jagged1-induced Notch signaling requires the action of beta 4galactosyltransferase-1</bp:TITLE>
    <bp:AUTHORS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chen, J</bp:AUTHORS>
